{
  "$jason": {
    "head": {
      "title": "image sample"
      },
      "actions": {
        "$load": {
          "type": "$set",
          "options": {
            "selected": ""
          },
          "success": {
            "type": "$render"
          }
        },
        "$pull": {
          "type": "$media.camera",
          "options": {
            "quality": "0.4"
          }
            }
          },
      "templates": {
        "body": {
          "style": {
            "border": "none"
          },
          "header": {
            "menu": {
              "text": "View Full Source",
              "href": {
                "url": "https://github.com/Jasonette/s3-upload-example/blob/master/app.js",
                "view": "web"
              }
            }
          },
          "sections": [
            {
              "style": {
                "spacing": "0",
                "padding": "0"
              },
              "header": {
                "type": "vertical",
                "style": {
                  "align": "center",
                  "padding": "20",
                  "z_index": "-1"
                },
                "components": [{
                  "type": "image",
                  "url": "https://d30y9cdsu7xlg0.cloudfront.net/png/126349-200.png",
                  "style": {
                    "z_index": "-1",
                    "width": "100"
                  }
                }]
              },
              "items": [
                {
                  "{{#if db && db.length > 0}}": {
                    "{{#each db}}": {
                      "type": "image",
                      "style": {
                        "width": "100%",
                        "padding": "0"
                      },
                      "url": "{{url}}"
                    }
                  }
                }, {
                  "{{#else}}": []
                }
              ]
            }
          ]
        }
      }
    }
  }
}